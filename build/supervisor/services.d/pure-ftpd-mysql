[program:pure-ftpd-mysql]
command=/usr/sbin/pure-ftpd-mysql-virtualchroot -p 30000:30000 -l mysql:/etc/pure-ftpd/db/mysql.conf -l pam -J ALL:!aNULL:!SSLv3 -A -u 1000 -D -H -E -O clf:/var/log/pure-ftpd/transfer.log -b -Y 1
stdout_logfile=/var/log/ftpserver.log
stderr_logfile=/var/log/ftpserver.log
autorestart=true
autostart=true
